{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/mutant.schema.json",
  "title": "Imperial Float",
  "description": "Imperial Float",
  "type": "object",
  "properties": {
    "distance": {
      "type": "number",
      "minimum": 0
    },
    "message": {
      "description": "Encrypt message",
      "type": "array",
      "minItems": 5,
      "items": {
        "type": "string",
        "minLength": 0,
        "customMessages": {
          "type": "Message must be a string"
        }
      }
    }
  },
  "required": ["distance","message"]
}
